<UserControl x:Class="CM3D2.MaidFiddler.WPF.TabControls.TabYotogi"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:CM3D2.MaidFiddler.WPF.TabControls"
             xmlns:t="clr-namespace:CM3D2.MaidFiddler.WPF.Translations"
             mc:Ignorable="d">
    <ScrollViewer CanContentScroll="True" VerticalScrollBarVisibility="Auto">
        <DataGrid Name="dgYotogi">
            <DataGrid.Columns>
                <DataGridCheckBoxColumn Header="{t:Translate HAS}" Width="Auto" />
                <DataGridTextColumn Header="{t:Translate NAME}" Width="*" IsReadOnly="True" />
                <DataGridTextColumn Header="{t:Translate LVL}" Width="50" />
                <DataGridTextColumn Header="{t:Translate TOTAL_XP}" Width="Auto" />
                <DataGridTextColumn Header="{t:Translate PLAY_COUNT}" Width="Auto" />
            </DataGrid.Columns>
        </DataGrid>
    </ScrollViewer>
</UserControl>