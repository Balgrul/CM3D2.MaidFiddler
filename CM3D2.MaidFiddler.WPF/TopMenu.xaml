            <MenuItem Header="{t:Translate MENU_MAID_FORCE_ENABLE}" />
            <MenuItem Header="{t:Translate MENU_MAID_WORK_FORCE_DISABLE}" />
            <MenuItem Header="{t:Translate MENU_MAID_LOCK_ALL}" />
            <MenuItem Header="{t:Translate MENU_MAID_UNLOCK_ALL}" />
        </MenuItem>
        <MenuItem Header="{t:Translate MENU_MISC}">
            <MenuItem Header="{t:Translate MENU_MISC_LANG_SELECT}" />
            <MenuItem Header="{t:Translate MENU_MISC_SETTINGS}" />
            <MenuItem Header="{t:Translate MENU_MISC_ABOUT}" />
        </MenuItem>
    </Menu>
</UserControl>