            <Separator />
            <MenuItem Header="{t:Translate MENU_ALL_MAID_ALL_SCENES_VISIBLE}" />
        </MenuItem>
        <MenuItem Header="{t:Translate MENU_MAID}">
            <MenuItem Header="{t:Translate MENU_MAID_UNLOCK}">
                <MenuItem Header="{t:Translate UNLOCK_SKILLS}" />
